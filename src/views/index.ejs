                            <div class="d-flex justify-content-center align-items-center mt-5">
                                <form action="/print" method="POST" class="form card-body border rounded needs-validation tamanho-form " novalidate
                                    id="form">
                                    <legend>
                                        Dados do Servidor
                                    </legend>
                                    <hr>

                                    <div>
                                        <label for="validacaoNome" class="form-label">Nome por Extenso</label>
                                        <input type="text" class="nome-extenso form-control" id="validacaoNome" required
                                            name="nome">
                                        <div class="invalid-feedback">
                                            Coloque seu nome.
                                        </div>

                                    </div>

                                    <div>
                                        <label for="naturalidade" class="form-label">Naturalidade</label>
                                        <input type="text" name="naturalidade" class="form-control" id="naturalidade"
                                            required>
                                        <div class="invalid-feedback">
                                            Coloque sua naturalidade.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="estado_civil" class="form-label">Estado Civil</label>
                                        <input type="text" name="estado_civil" class="form-control" id="estado_civil"
                                            required>
                                        <div class="invalid-feedback">
                                            Coloque seu Estado Civil.
                                        </div>
                                    </div>


                                    <div>
                                        <label for="endereco" class="form-label">Endereço</label>
                                        <input type="text" name="endereço" class="form-control" id="endereco" required>
                                        <div class="invalid-feedback">
                                            Coloque seu Endereço.
                                        </div>
                                    </div>


                                    <div>
                                        <label for="telefone" class="form-label">telefone</label>
                                        <input type="text" name="telefone" class="telefone form-control" id="telefone"
                                            required>
                                        <div class="invalid-feedback">
                                            Coloque seu telefone.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="bairro" class="form-label">Bairro</label>
                                        <input type="text" name="bairro" class="form-control" id="bairro" required>
                                        <div class="invalid-feedback">
                                            Coloque o nome do seu bairro.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="cep" class="form-label">CEP</label>
                                        <input type="text" name="cep" class="cep form-control" id="cep" required>
                                        <div class="invalid-feedback">
                                            Coloque o seu cep.
                                        </div>

                                    </div>

                                    <div>
                                        <label for="matricula" class="form-label">Matrícula</label>
                                        <input type="text" name="matricula" class="form-control" id="matricula"
                                            required>
                                        <div class="invalid-feedback">
                                            Coloque sua Matrícula.
                                        </div>
                                    </div>
                                    <div>
                                        <label for="cargo" class="form-label">Cargo</label>
                                        <input type="text" name="cargo" class="form-control" id="cargo" required>
                                        <div class="invalid-feedback">
                                            Coloque seu Cargo.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="identidade" class="form-label">Identidade</label>
                                        <input type="text" name="identidade" class="rg form-control" id="identidade" required>
                                        <div class="invalid-feedback">
                                            Coloque o número do seu RG.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="cpf" class="form-label">CPF</label>
                                        <input type="text" name="cpf" class="cpf form-control inputCPF" itemref="cpf" required>
                                        <!-- <div class="invalid-feedback">
                                            Coloque o número do seu CPF.
                                        </div> -->
                                        <div class="validaCPF"></div>
                                    </div>

                                    <div>
                                        <label for="data_nasc" class="form-label">Data de Nascimento</label>
                                        <input type="text" name="data_nasc" class="date form-control" id="data_nasc" required>
                                        <div class="invalid-feedback">
                                            Coloque sua data de Nascimento.
                                        </div>
                                    </div>

                                    <div>
                                        <label for="lotacao" class="form-label">Lotação</label>
                                        <input type="text" class="input-lotacao form-control" name="lotacao" id="lotacao" required>
                                        <div class="invalid-feedback">
                                            Coloque sua lotação
                                        </div>

                                    </div>

                                    <div>
                                        <label for="dados_requerimento" class="form-label">Venho respeitosamente, por meio deste documento,
                                            requerer o que se segue:
                                        </label>
                                        <textarea class="input-lotacao form-control" id="dados_requerimento" rows="5"
                                            name="dados_requerimento" required></textarea>
                                        <div class="invalid-feedback">
                                            Esse campo não pode estar vazio!
                                        </div>
                                    </div>
                                    

                                    <div class="text-right mt-3">
                                        <input type="submit" value="Enviar" name="btn-gerar"
                                            class="btn btn-primary ml-auto btnVerificar">

                                    </div>

                                </form>
                            </div>


                        </div>
                    </div>
                </div>
            </div>
        </div>










    </section>
    


